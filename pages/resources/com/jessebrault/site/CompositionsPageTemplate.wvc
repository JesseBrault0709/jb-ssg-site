---
package com.jessebrault.site

import com.jessebrault.site.composition.CompositionCategory
---
<StandardPage title={title}>
    <article>
        <h1>Compositions</h1>
        <Each
            items={categories}
            transform={<CompositionCategory
                category={it}
                compositions={compositions.getByCategory(it)}
            />}
        />
    </article>
</StandardPage>
