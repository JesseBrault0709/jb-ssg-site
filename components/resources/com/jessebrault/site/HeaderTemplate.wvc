---
package com.jessebrault.site

import com.jessebrault.site.icon.BarsIcon
---
<header>
    <div class="titles">
        <h1>$siteName</h1>
        <h2>$siteTagLine</h2>
    </div>
    <nav>
        <button class="bars-wrapper">
            <BarsIcon />
        </button>
        <ul>
            <Each items={menuItems} transform={<li><a href={it.path}>$it.name</a></li>} />
        </ul>
    </nav>
</header>
